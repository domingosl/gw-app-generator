version: '3'
services:
  <%- serviceName %>:
    container_name: <%- containerName %>
    restart: unless-stopped
    build:
      context: .
      dockerfile: Dockerfile
    ports:
      - '<%- applicationPort %>:<%- applicationPort %>'
<% if (ciServerPort) { -%>
      - '<%- ciServerPort %>:<%- ciServerPort %>'
<% } -%>
<% if (networkConnected) { -%>
networks:
  default:
    external:
      name: backend-services
<% } -%>